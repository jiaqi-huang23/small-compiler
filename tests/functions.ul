//string as para
boolean same(string a, string b) {
	if (a == b) {
		return true;
	} 
	return false;
}

//array as para
int arr(int[3] a) {
	if (a[2] < 0) {
		return a[1];
	} 
		return a[0];
	
}

//recursive function
int recur( int b) {
	if (b == 3) {
		return b;
	}
	return b + recur(b+1);
}

void main() {
	println same("abc", "abc");
	int[3] a;
	a[0] = 0;
	a[1] = 1;
	a[2] = 2;

	println arr(a);

	println "recursive";
	println recur(0);

	

}